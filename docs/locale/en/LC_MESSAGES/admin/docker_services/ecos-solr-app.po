#: ../../admin/docker_services/ecos-solr-app.rst:38
msgid "ALFRESCO_HOST - fqdn/ip инстанса ecos"
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:39
msgid "ALFRESCO_PORT - http порт инстанса ecos"
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:40
msgid "ALFRESCO_PORT_SSL - https порт инстанса ecos"
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:41
msgid "ALFRESCO_SECURE_COMMS - использовать шифрованное соединение"
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:42
msgid ""
"CITECK_MERGE_FACTOR - мерж фактор solr/lucene, используемый при "
"определении необходимости мержить сегменты."
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:43
msgid ""
"ALFRESCO_INDEX_TRANSFORM_CONTENT - если true - будет происходить "
"конвертация контента в текст и его последующая пословесная индексация. "
"Если false - будут индексироваться только метаданные (mimetype, size, "
"etc)."
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:44
msgid ""
"ALFRESCO_RECORD_UNINDEXED_NODES - если true - ноды, типы которых отмечены"
" как \"неиндексируемые\" - будут попадать в индекс в качестве документа "
"без индексации атрибутов ноды. Если false - такие документы будут "
"игнорироваться при индексации."
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:45
msgid ""
"CITECK_RECORD_TRANSACTIONS - если true - каждая транзакция будет попадать"
" в индекс, как отметка о том, что она проиндексирована. Если false - "
"данные о проиндексированных транзакциях будут храниться только в кеше, в "
"памяти."
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:46
msgid ""
"CITECK_TX_CONSISTENCY_CHECK_MODE - тип проверки консистентности индекса и"
" базы для индексации транзакций. Может принимать значения "
"FULL_DB_AND_INDEX_CHECK, ONLY_LAST_TRANSACTION или NONE."
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:47
msgid ""
"CITECK_TX_IS_INDEXED_CACHE_SIZE - размер кеша, если "
"CITECK_RECORD_TRANSACTIONS = false."
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:48
msgid ""
"CITECK_TX_IS_INDEXED_CACHE_CLEAR_COEFFICIENT - коэффициент чистки кеша "
"при переполнении, если CITECK_RECORD_TRANSACTIONS = false."
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:49
msgid ""
"CITECK_RECORD_ACL_TRANSACTIONS - если true - каждая транзакция прав будет"
" попадать в индекс, как отметка о том, что она проиндексирована. Если "
"false - данные о проиндексированных транзакциях прав будут храниться "
"только в кеше, в памяти."
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:50
msgid ""
"CITECK_ACL_CONSISTENCY_CHECK_MODE - тип проверки консистентности индекса "
"и базы для индексации транзакций прав. Может принимать значения "
"FULL_DB_AND_INDEX_CHECK, ONLY_LAST_TRANSACTION или NONE."
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:51
msgid ""
"CITECK_ACL_TX_IS_INDEXED_CACHE_SIZE - размер кеша, если "
"CITECK_RECORD_ACL_TRANSACTIONS = false."
msgstr ""

#: ../../admin/docker_services/ecos-solr-app.rst:52
msgid ""
"CITECK_ACL_TX_IS_INDEXED_CACHE_CLEAR_COEFFICIENT - коэффициент чистки "
"кеша при переполнении, если CITECK_RECORD_ACL_TRANSACTIONS = false."
msgstr ""

